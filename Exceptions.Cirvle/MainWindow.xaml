<Window x:Class="Exceptions.Circle.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Exceptions.Cirvle"
        mc:Ignorable="d"
        Title="Exceptions" Height="450" Width="800">
    <Grid>
        <Button x:Name="ohne" Content="Ohne Fehlerbehandlung" HorizontalAlignment="Left" Margin="240,68,0,0" VerticalAlignment="Top" Width="290" Height="59" Click="ohne_Click"/>
        <Button x:Name="mit" Content="Mit Fehlerbehandlung" HorizontalAlignment="Left" Margin="240,132,0,0" VerticalAlignment="Top" Width="290" Height="59" Click="mit_Click"/>
        <Button x:Name="mehr" Content="Behandlung mehrerer Fehler" HorizontalAlignment="Left" Margin="240,196,0,0" VerticalAlignment="Top" Width="290" Height="59" Click="mehr_Click"/>
        <Button x:Name="konver" Content="Beispiel Zahl Konvertierung" HorizontalAlignment="Left" Margin="240,260,0,0" VerticalAlignment="Top" Width="290" Height="59" Click="konver_Click"/>
        <Button x:Name="circle" Content="Beispiel Cicle" HorizontalAlignment="Left" Margin="240,324,0,0" VerticalAlignment="Top" Width="290" Height="59" Click="circle_Click"/>
    </Grid>
</Window>
